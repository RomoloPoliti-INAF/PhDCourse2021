   "outputs": [],
   "source": [
    "def myDec(func):\n",
    "    def inner(*args, **keys):\n",
    "        print(\"###############\")\n",
    "        func(*args,**keys)\n",
    "        print(\"###############\")\n",
    "    return inner"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "3006e24e",
   "metadata": {},
   "outputs": [],
   "source": [
    "@myDec\n",
    "def myFunc(arg):\n",
    "    print(f\"Hello {arg}\")\n"
   ]
  },
